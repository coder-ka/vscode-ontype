{
  "name": "vscode-ontype",
  "displayName": "ontype",
  "version": "1.0.8",
  "description": "VSCode extension for ontype lsp client",
  "main": "dist/index.js",
  "engines": {
    "vscode": "^1.94.0"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "activationEvents": [
    "onLanguage:ontype"
  ],
  "contributes": {
    "languages": [
      {
        "id": "ontype",
        "extensions": [
          ".ontype"
        ]
      }
    ]
  },
  "scripts": {
    "build": "tsx build-scripts/build.ts",
    "watch": "tsx build-scripts/watch.ts",
    "start": "node dist",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "login": "vsce login coder-ka"
  },
  "publisher": "coder-ka",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/coder-ka/vscode-ontype.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/coder-ka/vscode-ontype/issues"
  },
  "homepage": "https://github.com/coder-ka/vscode-ontype#readme",
  "devDependencies": {
    "@types/node": "^18.11.9",
    "@types/vscode": "^1.94.0",
    "@vscode/vsce": "^3.6.2",
    "chokidar": "^4.0.1",
    "esbuild": "^0.15.15",
    "ontype-lsp-server": "^1.0.2",
    "tsx": "^4.19.1"
  },
  "dependencies": {
    "vscode-languageclient": "^9.0.1"
  }
}
